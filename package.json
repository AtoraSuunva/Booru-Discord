{
  "name": "boorubot",
  "version": "4.7.0",
  "description": "A Discord bot to search boorus",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc --build && node --env-file=.env dist/index.js",
    "start:prod": "prisma migrate deploy && node --enable-source-maps dist/index.js",
    "build": "tsc --build",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --write && prisma format && tsc --noEmit",
    "sentry:sourcemaps:inject": "sentry-cli sourcemaps inject ./dist",
    "sentry:sourcemaps:upload": "sentry-cli sourcemaps upload ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/BooruBot.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/BooruBot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/BooruBot#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "@prisma/instrumentation": "^6.2.1",
    "booru": "^2.7.0",
    "discord.js": "^14.17.3",
    "env-var": "^7.5.0",
    "pino": "^9.6.0",
    "pino-dev": "^4.0.3",
    "pretty-ms": "^9.2.0",
    "prisma": "^6.2.1",
    "sleetcord": "^2.3.1",
    "sleetcord-common": "^11.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sentry/cli": "^2.41.1",
    "@types/node": "^22.10.10",
    "typescript": "^5.7.3"
  }
}
