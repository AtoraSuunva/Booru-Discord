{
  "name": "boorubot",
  "version": "4.2.3",
  "description": "A Discord bot to search boorus",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc --build && node -r dotenv/config --trace-warnings dist/index.js",
    "start:prod": "npx prisma migrate deploy && node dist/index.js",
    "build": "tsc --build",
    "lint": "eslint src/ && prettier --check src/ && tsc --noEmit",
    "lint:fix": "eslint src/ --fix && prettier --write src/ && pnpm prisma format && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtoraSuunva/BooruBot.git"
  },
  "keywords": [],
  "author": "AtoraSuunva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtoraSuunva/BooruBot/issues"
  },
  "homepage": "https://github.com/AtoraSuunva/BooruBot#readme",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "booru": "^2.6.3",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "env-var": "^7.3.1",
    "pino": "^8.15.0",
    "pino-dev": "^4.0.2",
    "pretty-ms": "^8.0.0",
    "rollbar": "^2.26.2",
    "sleetcord": "^0.16.0",
    "sleetcord-common": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6"
  }
}
